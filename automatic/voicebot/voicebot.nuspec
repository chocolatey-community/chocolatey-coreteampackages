<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>voicebot</id>
    <version>0.0</version>
    <title>VoiceBot</title>
    <authors>Binary Fortress Software</authors>
    <projectUrl>https://www.voicebot.net</projectUrl>
    <licenseUrl>https://www.voicebot.net/License/</licenseUrl>
    <iconUrl>https://images.binaryfortress.com/ChocolateyIcons/VoiceBot.png</iconUrl>
