    <requireLicenseAcceptance>false</requireLicenseAcceptance>
	<iconUrl>https://cdn.rawgit.com/adgellida/chocolateyautomaticpackages/master/icons/{{PackageName}}.png</iconUrl>
    <dependencies>
    <dependency id="autoit.commandline" />
    </dependencies>
  </metadata>
</package>